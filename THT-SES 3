// 1. Problem Statement : Write a program that accepts a number, n, and outputs the same
#include<stdio.h>

int main()
{
  int n;
  
  scanf("%d", &n);
  printf("%d\n", n);
  
  return 0;
}

// 2. Problem statement : Write a program to take a character (C) as input and check whether the given character is a vowel or a consonant
#include <stdio.h>

int main() 
{
	char c;
	scanf("%c", &c);
	
	if (c == 'A' || c == 'E'|| c == 'I'|| c == 'O'||c == 'U')
	
	{printf("Vowel\n");} 
	
	else 
	{printf("Consonant\n");}
	
	return 0;
}

/* 3. Problem statement : Write a program to obtain length (L) and breadth (B) of a rectangle and 
      check whether its area is greater or perimeter is greater or both are equal.*/
#include <stdio.h>

int main() 
{
	int length, breadth, peri, area;
	scanf("%d\n%d", &length, &breadth);
	
	peri=2*(length+breadth);
	area=(length*breadth);
	
	if(area>peri)
	{
		printf("Area\n");
		printf("%d", area);
	}
	else if(peri>area){
		printf("Peri\n");
		printf("%d", peri);
	}
	else{
		printf("Eq\n");
		printf("%d", area);
	}

	return 0;
}

/* 4. Problem statement : Write a program to obtain a number (N) from the user and display whether the 
      number is a one digit number, 2 digit number, 3 digit number or more than 3 digit number. */
#include <stdio.h>

int main() {
	int number;
	scanf("%d",&number);
	
	if(number>=0 && number<=9){
		printf("1");
	}
	else if(number>=10 && number<=99){
		printf("2");
	}
	else if(number>=100 && number<=999){
		printf("3");
	}
	else if(number>=1000 && number<=1000000){
		printf("More than 3 digits");
	}
	
	return 0;
}

// 5. Write a program to take two numbers as input and print their difference if the first number is greater than the second number  otherwise  print their sum.
#include <stdio.h>

int main () {
	int N1;
	int N2;
	scanf("%d", &N1);
	scanf("%d", &N2);
	if(N1 > N2 ){printf ("%d", N1-N2);
	}
	else if(N1 <= N2){printf ("%d", N1+N2);
	}
	return 0;
}
